<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EAF4FE">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="10dp"
                app:cardElevation="3dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            style="@style/RobotoMedium"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="Inventory Management"
                            android:textAlignment="center"
                            android:textStyle="bold"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/eighteen" />


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fifteen"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/home" />

                        <TextView
                            android:id="@+id/homepage"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="5dp"
                            android:text="Home"
                            android:textAlignment="gravity"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/sixteen" />


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_sales"
                            app:expandable_secondLayout="@layout/nav_expandable_child_sales"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_delivery"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_delivery_parent"
                            app:expandable_secondLayout="@layout/nav_expandable_child_delivery"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_due_advance_collect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_due_advance_collect">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_purchase"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_purchase"
                            app:expandable_secondLayout="@layout/nav_expandable_child_purchase"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_customer_ledger"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_customer_ledger">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_supplier"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_supplier"
                            app:expandable_secondLayout="@layout/nav_expandable_child_supplier"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_product_inventory"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_product_inventory"
                            app:expandable_secondLayout="@layout/nav_expandable_child_product_inventory"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_expense"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_expense"
                            app:expandable_secondLayout="@layout/nav_expandable_child_expense"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_fund"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_fund"
                            app:expandable_secondLayout="@layout/nav_expandable_child_fund"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_report"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_report"
                            app:expandable_secondLayout="@layout/nav_expandable_child_report"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight">

                        <com.skydoves.expandablelayout.ExpandableLayout
                            android:id="@+id/expandable_settings"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:expandable_duration="300"
                            app:expandable_isExpanded="false"
                            app:expandable_parentLayout="@layout/nav_expandable_parent_settings"
                            app:expandable_secondLayout="@layout/nav_expandable_child_settings"
                            app:expandable_showSpinner="true"
                            app:expandable_spinner="@drawable/ic_arrow_down"
                            app:expandable_spinner_animate="true"
                            app:expandable_spinner_color="#000"
                            app:expandable_spinner_gravity="end"
                            app:expandable_spinner_margin="14dp"
                            app:expandable_spinner_size="32dp">

                        </com.skydoves.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/eight"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/logout" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Logout"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/sixteen" />

                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/ten">

                <TextView
                    style="@style/RobotoBold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:padding="5dp"
                    android:text="Communication"
                    android:textStyle="bold"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/sixteen" />

            </LinearLayout>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="10dp"
                app:cardElevation="3dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/like" />

                        <TextView
                            style="@style/RobotoBold"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="8dp"
                            android:text="Rate"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/sixteen" />


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC"
                        app:layout_constraintTop_toBottomOf="@+id/play" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/share" />

                        <TextView
                            style="@style/RobotoBold"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="8dp"
                            android:text="Share"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/sixteen" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC"
                        app:layout_constraintTop_toBottomOf="@+id/play" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/privacy" />

                        <TextView
                            style="@style/RobotoBold"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="8dp"
                            android:text="Privacy Policy"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/sixteen" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/colorC"
                        app:layout_constraintTop_toBottomOf="@+id/play" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/terms" />

                        <TextView
                            style="@style/RobotoBold"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="8dp"
                            android:text="Terms Of Service"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/sixteen" />

                    </LinearLayout>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>


    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>